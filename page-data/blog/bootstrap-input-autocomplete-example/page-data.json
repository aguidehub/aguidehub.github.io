pan> availableTags <span class=\"token operator\">=</span> <span class=\"token punctuation\">[</span>\n                <span class=\"token string\">\"ActionScript\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"AppleScript\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Asp\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"BASIC\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"C\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"C++\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Clojure\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"COBOL\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"ColdFusion\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Erlang\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Fortran\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Groovy\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Haskell\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Java\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"JavaScript\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Lisp\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Perl\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"PHP\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Python\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Ruby\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Scala\"</span><span class=\"token punctuation\">,</span>\n                <span class=\"token string\">\"Scheme\"</span>\n            <span class=\"token punctuation\">]</span><span class=\"token punctuation\">;</span>\n            <span class=\"token function\">$</span><span class=\"token punctuation\">(</span><span class=\"token string\">\"#tags\"</span><span class=\"token punctuation\">)</span><span class=\"token punctuation\">.</span><span class=\"token function\">autocomplete</span><span class=\"token punctuation\">(</span><span class=\"token punctuation\">{</span>\n                <span class=\"token literal-property property\">source</span><span class=\"token operator\">:</span> availableTags\n            <span class=\"token punctuation\">}</span><span class=\"token punctuation\">)</span><span class=\"token punctuation\">;</span>\n        <span class=\"token punctuation\">}</span><span class=\"token punctuation\">)</span><span class=\"token punctuation\">;</span>\n    </span></span><span class=\"token tag\"><span class=\"token tag\"><span class=\"token punctuation\">&lt;/</span>script</span><span class=\"token punctuation\">></span></span>\n\n    <span class=\"token comment\">&lt;!-- Option 1: Bootstrap Bundle with Popper --></span>\n    <span class=\"token tag\"><span class=\"token tag\"><span class=\"token punctuation\">&lt;</span>script</span> <span class=\"token attr-name\">src</span><span class=\"token attr-value\"><span class=\"token punctuation attr-equals\">=</span><span class=\"token punctuation\">\"</span>https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js<span class=\"token punctuation\">\"</span></span>\n        <span class=\"token attr-name\">integrity</span>